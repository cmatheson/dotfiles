[user]
	name = Cameron Matheson
	useConfigOnly = true
[alias]
	st = status --short --branch
	co = checkout
	br = branch --sort=committerdate
	sdiff = diff --staged
	ci = commit
	glog = log --graph
  fmerge = merge --ff-only
	olog = log --oneline
	amend = commit --amend
	newbranch = checkout -t origin/master -b
	cleanup = !git br | fzf --multi  | xargs git br -D
[color]
	ui = true
[rerere]
	enabled = 1
[init]
	templatedir = ~/.git_template
[push]
	default = simple
[pull]
	rebase = true
[diff]
	noprefix = true
